{
  "name": "pdfjs-react-viewer",
  "version": "0.1.0-15",
  "description": "A React component library for viewing PDFs using PDF.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist/types"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build:package": "rimraf dist && tsup",
    "build:demo": "rimraf demo-dist && tsc -b && vite build --config vite.demo.config.ts",
    "lint": "eslint .",
    "preview": "vite preview --config vite.demo.config.ts"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3"
  }
}
